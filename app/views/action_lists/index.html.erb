<h1>Listing action_lists</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Goal</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @action_lists.each do |action_list| %>
  <tr>
    <td><%= action_list.title %></td>
    <td><%= action_list.goal %></td>
    <td><%= link_to 'Show', action_list %></td>
    <td><%= link_to 'Edit', edit_action_list_path(action_list) %></td>
    <td><%= link_to 'Destroy', action_list, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

